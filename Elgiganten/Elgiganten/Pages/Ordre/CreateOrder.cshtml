@page
@model Elgiganten.Pages.Ordre.CreateOrderModel.CreateOrdreModel
@{
}

<form method="post">
    <div class="form-group">
        <label asp-for="ordre.Id"></label>
        <input asp-for="ordre.Id" class="form-control" />
        <span asp-validation-for="ordre.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ordre.Item"></label>
        <input asp-for="ordre.Item" class="form-control" />
        <span asp-validation-for="ordre.Item" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ordre.Customer"></label>
        <input asp-for="ordre.Customer" class="text-danger" />
        <span asp-validation-for="ordre.Customer" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ordre.OrdreDate"></label>
        <input asp-for="ordre.OrdreDate" class="text-danger" />
        <span asp-validation-for="ordre.OrdreDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ordre.Quantity"></label>
        <input asp-for="ordre.Quantity" class="text-danger" />
        <span asp-validation-for="ordre.Quantity" class="text-danger"></span>
    </div>
   
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
